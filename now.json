{
  "version": 2,
  "name": "domus",
  "build": {
    "src": "package.json",
    "use": "@now/static-build",
    "config": { "distDir": "public" },
    "env": {
      "API_KEY": "@prismic-api",
      "GOOGLE_ANALYTICS_ID": "@oogle-analytics-id"
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "/404" }
  ]
}
